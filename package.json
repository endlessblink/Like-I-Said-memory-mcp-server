{
  "name": "@endlessblink/like-i-said-v2",
  "version": "2.2.3",
  "description": "Enhanced MCP memory server with GitHub integration and modern React dashboard - remember conversations across sessions",
  "main": "server-markdown.js",
  "type": "module",
  "keywords": [
    "mcp",
    "memory",
    "ai",
    "claude",
    "cursor",
    "windsurf",
    "assistant",
    "context",
    "github",
    "dashboard",
    "react",
    "integration"
  ],
  "author": "endlessblink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endlessblink/Like-I-Said-Memory-V2.git"
  },
  "homepage": "https://github.com/endlessblink/Like-I-Said-Memory-V2",
  "bugs": {
    "url": "https://github.com/endlessblink/Like-I-Said-Memory-V2/issues"
  },
  "bin": {
    "like-i-said-v2": "./cli.js"
  },
  "files": [
    "cli.js",
    "cli.cmd",
    "server-markdown.js",
    "backup-system.js",
    "backup-runner.js",
    "dashboard-server.js",
    "dist",
    "src",
    "public",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "node server-markdown.js",
    "start:mcp": "node server-markdown.js",
    "start:legacy": "node server.js",
    "start:dashboard": "node dashboard-server.js",
    "dev": "vite --host 0.0.0.0",
    "dev:full": "concurrently \"npm run start:dashboard\" \"npm run dev\" --names \"API,UI\" --prefix-colors \"green,magenta\"",
    "build": "vite build",
    "preview": "vite preview",
    "install-mcp": "node cli.js install",
    "configure": "node cli.js init",
    "test:mcp": "echo '{\"jsonrpc\": \"2.0\", \"id\": 1, \"method\": \"tools/list\"}' | node server-markdown.js",
    "test:api": "curl -s http://localhost:3001/api/status"
  },
  "dependencies": {
    "@endlessblink/like-i-said-v2": "^2.1.9",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/cytoscape": "^3.21.9",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compressing": "^1.10.3",
    "cors": "^2.8.5",
    "cytoscape": "^3.32.0",
    "d3": "^7.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.27.1",
    "react-force-graph-3d": "^1.27.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.177.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.21",
    "puppeteer": "^24.10.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.4.9"
  }
}
