{
  "name": "@endlessblink/like-i-said-v2",
  "version": "2.6.12",
  "description": "Enhanced MCP memory server with modern React dashboard - remember conversations across sessions",
  "main": "mcp-server-wrapper.js",
  "type": "module",
  "keywords": [
    "mcp",
    "memory",
    "ai",
    "claude",
    "cursor",
    "windsurf",
    "assistant",
    "context",
    "github",
    "dashboard",
    "react",
    "integration"
  ],
  "author": "endlessblink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endlessblink/Like-I-Said-memory-mcp-server.git"
  },
  "homepage": "https://github.com/endlessblink/Like-I-Said-memory-mcp-server",
  "bugs": {
    "url": "https://github.com/endlessblink/Like-I-Said-memory-mcp-server/issues"
  },
  "bin": {
    "like-i-said-v2": "./cli.js",
    "like-i-said-v2-mcp": "./npx-clean-wrapper.js"
  },
  "files": [
    "cli.js",
    "mcp-server-wrapper.js",
    "npx-clean-wrapper.js",
    "server-markdown.js",
    "dashboard-server-bridge.js",
    "lib",
    "dist",
    "src",
    "public",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "node mcp-server-wrapper.js",
    "start:mcp": "node mcp-server-wrapper.js",
    "start:legacy": "node server.js",
    "start:dashboard": "node dashboard-server-bridge.js",
    "dev": "cross-env CHOKIDAR_USEPOLLING=true vite --host 0.0.0.0",
    "dev:full": "concurrently \"npm run start:dashboard\" \"npm run dev\" --names \"API,UI\" --prefix-colors \"green,magenta\"",
    "dashboard": "concurrently \"npm run start:dashboard\" \"npm run dev\" --names \"API,UI\" --prefix-colors \"green,magenta\"",
    "build": "vite build",
    "preview": "vite preview",
    "migrate": "node scripts/migrate-memory-formats.js",
    "migrate:neo4j": "node scripts/migrate-to-neo4j.js",
    "neo4j:start": "docker-compose -f docker-compose.neo4j.yml up -d",
    "neo4j:stop": "docker-compose -f docker-compose.neo4j.yml down",
    "neo4j:test": "node scripts/test-neo4j.js",
    "install-mcp": "node cli.js install",
    "configure": "node cli.js init",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mcp": "echo '{\"jsonrpc\": \"2.0\", \"id\": 1, \"method\": \"tools/list\"}' | node server-markdown.js",
    "test:api-curl": "curl -s http://localhost:3001/api/status",
    "test:syntax": "node tests/syntax-validation.js",
    "test:comprehensive": "node tests/comprehensive-mcp-test-suite.js",
    "test:protocol": "node tests/json-rpc-protocol-test.js",
    "test:persistence": "node tests/memory-persistence-test.js",
    "test:validation": "node tests/final-comprehensive-validation.js",
    "test:demo": "node tests/demo-working-server.js",
    "test:all": "node tests/run-all-tests.js",
    "test:pre-push": "node tests/pre-push-validation.js",
    "test:api": "node tests/api-integration-test.js",
    "test:ui": "node tests/ui-layout-test.js", 
    "test:websocket": "node tests/websocket-reconnection-test.js",
    "test:integration": "npm run test:api && npm run test:ui",
    "test:debug": "node tests/debug-data-loading.js",
    "test:data-issue": "node tests/debug-data-issue.js",
    "fix:paths": "node tests/fix-path-issue.js",
    "test:port-detection": "node tests/test-port-detection.js",
    "test:robust-systems": "node tests/test-robust-systems.js",
    "test:ui-safe-areas": "node tests/test-ui-safe-areas.js",
    "test:ui-visual": "node tests/test-ui-visual.js",
    "test:full-integration": "node tests/test-full-integration.js",
    "test:pre-release": "npm run test:robust-systems && npm run test:ui-safe-areas && npm run test:full-integration",
    "test:everything": "node tests/test-everything-works.js",
    "test:complete": "node tests/test-complete-system.js",
    "test:ui-connection": "node tests/test-ui-api-connection.js",
    "test:production-ready": "npm run test:everything && npm run test:ui-connection",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:standalone": "node mcp-server-standalone.js",
    "build:dxt": "node scripts/build-dxt.js",
    "validate:dxt": "node scripts/validate-dxt.js",
    "export:data": "node scripts/export-user-data.js",
    "import:data": "node scripts/import-user-data.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@xenova/transformers": "^2.17.2",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.6",
    "jest": "^30.0.4",
    "postcss": "^8.4.21",
    "puppeteer": "^24.10.1",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.4.9"
  }
}
