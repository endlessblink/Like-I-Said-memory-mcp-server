{
  "name": "like-i-said-dashboard-auto",
  "version": "2.4.3",
  "bin": "dashboard-launcher-auto-port.js",
  "pkg": {
    "scripts": [
      "dashboard-server-bridge.js",
      "lib/**/*.js",
      "src/**/*.js"
    ],
    "assets": [
      "dist/**/*",
      "public/**/*",
      "node_modules/@modelcontextprotocol/**/*",
      "node_modules/js-yaml/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64", 
      "node18-linux-x64"
    ],
    "outputPath": "dist-installer-auto"
  }
}